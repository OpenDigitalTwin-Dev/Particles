@Input Flux;
Flux.setGlossaryName("NeutronFlux");
Flux.setDepth(1);

@Output Fluence;
Fluence.setGlossaryName("NeutronFluence");
Fluence.setDefaultInitialValue(0.);
Fluence.setDepth(1);

@Function Compute
{    
  Fluence = Fluence_1+(0.5*(Flux_1+Flux)*dt);
}
